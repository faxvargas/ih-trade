
<h2>Cree una skin</h2>
<form action="create" method="POST">
<label>Skin Name</label>
<input
    type="text"
    name="nombreDelaSkin"
/>
<label>Descripcion</label>
<textarea
    type="text"
    name="descripcion"
></textarea>
<label>Juego</label>
<select name=
"juegoAlQuePertenece">
    {{#each juegos}}
<option value="{{this}}">
    {{this}}
</option>
{{/each}}
</select>
<label>Calidad</label>
<input 
      type="number"
      name="calidad"
      />
<label>Imagen</label>
<input
     type="text"
     name = "imagen"
     />

     <button type="submit">Crear Skin</button>
</form>
